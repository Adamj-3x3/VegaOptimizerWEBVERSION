{
  "builds": [
    {
      "src": "api/analyze_bullish_light.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/analyze_bearish_light.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/analyze_bullish",
      "dest": "/api/analyze_bullish_light.py"
    },
    {
      "src": "/api/analyze_bearish",
      "dest": "/api/analyze_bearish_light.py"
    }
  ]
}
